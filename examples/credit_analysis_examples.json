  "curl_examples": [
    {
      "name": "Exemplo de requisição cURL - Cliente Aprovado",
      "command": "curl -X POST http://localhost:8000/api/credit/analyze \\\n+  -H \"Content-Type: application/json\" \\\n+  -d '{\n    \"customer_profile\": {\n      \"customer_id\": \"CUST-001\",\n      \"name\": \"João Silva\",\n      \"age\": 35,\n      \"gender\": \"male\",\n      \"marital_status\": \"married\",\n      \"employment_status\": \"employed\",\n      \"income\": 8500.0,\n      \"debt_to_income_ratio\": 0.25,\n      \"credit_score\": 780,\n      \"time_at_job_months\": 48,\n      \"has_bank_account\": true,\n      \"has_bacen_restriction\": false,\n      \"num_credit_inquiries\": 2,\n      \"num_existing_loans\": 1\n    },\n    \"product_type\": \"personal_loan\",\n    \"requested_amount\": 25000.0,\n    \"requested_installments\": 36,\n    \"purpose\": \"home_improvement\"\n  }'"
    },
    {
      "name": "Listar produtos disponíveis",
      "command": "curl -X GET http://localhost:8000/api/credit/products"
    },
    {
      "name": "Health check do serviço de crédito",
      "command": "curl -X GET http://localhost:8000/api/credit/health"
    }
  ],
  "api_documentation": {
    "base_url": "http://localhost:8000",
    "swagger_ui": "http://localhost:8000/docs",
    "redoc": "http://localhost:8000/redoc",
    "endpoints": {
      "analyze_credit": {
        "method": "POST",
        "path": "/api/credit/analyze",
        "description": "Executa análise completa de crédito em 4 etapas (DFS, BFS, Fuzzy Logic, Neural Network)",
        "request_schema": "customer_profile + product_type + requested_amount + requested_installments + purpose",
        "response_schema": "CreditAnalysisResponseDTO"
      },
      "list_products": {
        "method": "GET",
        "path": "/api/credit/products",
        "description": "Lista produtos de crédito disponíveis com configurações"
      },
      "health_check": {
        "method": "GET",
        "path": "/api/credit/health",
        "description": "Verifica status do serviço de análise de crédito"
      }
    }
  }
}
